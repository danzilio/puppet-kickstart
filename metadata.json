{
  "name": "danzilio-kickstart",
  "version": "0.4.0",
  "author": "David Danzilio",
  "summary": "A module to generate Kickstart configurations with Puppet.",
  "license": "Apache-2.0",
  "source": "https://github.com/danzilio/puppet-kickstart",
  "project_page": "https://github.com/danzilio/puppet-kickstart",
  "issues_url": "https://github.com/danzilio/puppet-kickstart/issues",
  "tags": [
    "kickstart"
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
